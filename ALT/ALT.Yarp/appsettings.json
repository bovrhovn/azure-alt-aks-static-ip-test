{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Yarp": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "InstrumentationKey=YOUR_KEY_HERE"
  },
  "ReverseProxy": {
    "Routes": {
      "akstovmss": {
        "ClusterId": "production-cluster",
        "Match": {
          "Path": "{**catch-all}",
          "Headers": [
            {
              "Name": "redirect-allowed",
              "Mode": "Exists"
            }
          ]
        }
      }
    },
    "Clusters": {
      "production-cluster": {
        "Destinations": {
          "destination-api": {
            "Address": "https://localhost:5055/"
          }
        }
      }
    }
  }
}
