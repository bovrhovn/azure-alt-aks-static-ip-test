{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "InstrumentationKey=YOUR_KEY_HERE"
  },
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "to-production",
        "ClusterId": "production-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    ],
    "Clusters": [
      {
        "ClusterId": "production-cluster",
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:5055/"
          }
        }
      }
    ]
  }
}
